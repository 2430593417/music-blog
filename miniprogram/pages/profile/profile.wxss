page {
  background-color: #f1f1f1;
}
.profile-header {
  background-image: url(data:image/jpeg;base64,);  
  /*小程序的background只支持网络图片和base64，不支持本地图片*/
  background-size: cover;
  height: 480rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}
.avatar-url {
  width: 200rpx;
  height: 200rpx;
  display: block;
  overflow: hidden;
  border: 6rpx solid #fff;
  border-radius: 50%;
}
.nav {
  background-color: #fff;
  margin: 40rpx 24rpx 0rpx;
  border-radius: 20rpx;
}
.nav-item {
  padding: 20rpx 30rpx;
  border-bottom: 2rpx solid #f1f1f1;
}
.content {
  width: 100%;
  height: 70rpx;
  display: flex;
  justify-content: center;
  color: red;
}
.text {
  flex: 1;
  line-height: 70rpx;
  font-size: 36rpx;
  font-weight: bold;
  padding-left: 40rpx;
  color: #CD0000;
}
.left {
  float: left;
  line-height: 70rpx;
}
.right {
  float: right;
  line-height: 70rpx;
}